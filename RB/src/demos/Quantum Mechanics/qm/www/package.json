{
	"name": "create-wasm-app",
	"version": "0.1.0",
	"description": "create an app to consume rust-generated wasm packages",
	"main": "index.js",
	"bin": {
		"create-wasm-app": ".bin/create-wasm-app.js"
	},
	"scripts": {
		"clean-dist": "rm -rf ./dist/*",
		"clean-dist-dev": "rm -rf ./dist-dev/*",
		"build": "npm run clean-dist && webpack --config webpack.config.js",
		"build-dev": "npm run clean-dist-dev && webpack --config webpack.config.dev.js",
		"start": "webpack-dev-server--output-path dist --open",
		"start-dev": "webpack-dev-server --output-path dist-dev --open",
		"all": "wasm-pack build --target bundler && npm run build && npm run start",
		"all-dev": "wasm-pack build --target bundler && npm run build-dev && npm run start-dev"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rustwasm/create-wasm-app.git"
	},
	"keywords": [
		"webassembly",
		"wasm",
		"rust",
		"webpack"
	],
	"author": "Ashley Williams <ashley666ashley@gmail.com>",
	"license": "(MIT OR Apache-2.0)",
	"bugs": {
		"url": "https://github.com/rustwasm/create-wasm-app/issues"
	},
	"homepage": "https://github.com/rustwasm/create-wasm-app#readme",
	"dependencies": {
		"d3": "^7.6.1",
		"d3-array": "^3.2.0",
		"katex": "^0.16.0",
		"qm": "file:../pkg",
		"three": "^0.123.0"
	},
	"devDependencies": {
		"copy-webpack-plugin": "^11.0.0",
		"css-loader": "^6.7.1",
		"glob": "^7.2.3",
		"html-webpack-plugin": "^5.5.0",
		"qm": "^0.1.0",
		"style-loader": "^3.3.1",
		"webpack": "^5.1.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.10.0"
	}
}
