{{!-- https://docs.mathjax.org/en/latest/web/configuration.html --}}
{{!-- mathjax --}}
<script async>
    window.MathJax = {
        tex: {
            inlineMath: [['\\(', '\\)']],
            macros: {
                v: ["\\mathbf{#1}", 1],
            }
        },
        svg: {
            fontCache: 'global'
        }
    };

    (function () {
        var script = document.createElement('script');
        script.src = 'https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-svg-full.js';
        script.async = true;
        document.head.appendChild(script);
    })();
</script>
